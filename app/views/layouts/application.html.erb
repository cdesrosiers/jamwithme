<!DOCTYPE html>
<html>
<head>
  <title>Jamwithme</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
  JamWithMe
  <% if signed_in? %>
    <div id="username">
      <%= render 'users/menu', username: current_user.username %>
    </div>
  <% end %>
</header>
<%= yield %>

</body>
</html>
